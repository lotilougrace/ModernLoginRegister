<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">


    <TextView
        android:id="@+id/text_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="179dp"
        android:layout_marginTop="75dp"
        android:layout_marginRight="173dp"
        android:layout_marginBottom="637dp"
        android:text="Address" />

    <EditText
        android:id="@+id/edit_email"
        android:layout_width="182dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="121dp"
        android:layout_marginTop="253dp"
        android:layout_marginRight="108dp"
        android:layout_marginBottom="446dp"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/edit_mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/edit_address"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="123dp"
        android:layout_marginTop="84dp"
        android:layout_marginRight="110dp"
        android:layout_marginBottom="520dp"
        android:ems="10"
        android:inputType="phone" />

    <EditText
        android:id="@+id/edit_address"
        android:layout_width="179dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="123dp"
        android:layout_marginTop="99dp"
        android:layout_marginRight="109dp"
        android:layout_marginBottom="605dp"
        android:ems="10"
        android:inputType="textPostalAddress" />

    <TextView
        android:id="@+id/text_mobile"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="161dp"
        android:layout_marginTop="149dp"
        android:layout_marginRight="140dp"
        android:layout_marginBottom="563dp"
        android:text="Mobile Number" />

    <TextView
        android:id="@+id/text_email"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="163dp"
        android:layout_marginTop="219dp"
        android:layout_marginRight="131dp"
        android:layout_marginBottom="486dp"
        android:text="Email Address" />

    <Button
        android:id="@+id/update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="175dp"
        android:layout_marginTop="407dp"
        android:layout_marginRight="142dp"
        android:layout_marginBottom="276dp"
        android:text="Update"
        app:backgroundTint="@android:color/darker_gray" />
</RelativeLayout>